{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "LoginResponse.schema.json",
  "title": "LoginResponse",
  "description": "Response to login request containing JWT token",
  "type": "object",
  "properties": {
    "token": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_-]{2,}(?:\\.[A-Za-z0-9_-]{2,}){2}$"
    }
  },
  "required": [
    "token"
  ]
}
